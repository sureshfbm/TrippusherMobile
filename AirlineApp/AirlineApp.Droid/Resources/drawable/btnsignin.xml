<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#992f2f" />
      <stroke
          android:width="1dp"
          android:color="#992f2f" />
      <corners
          android:radius="4dp" />

    </shape>
  </item>
  <item>
    <shape>
      <gradient
          android:startColor="#C46E6E"
          android:endColor="#C46E6E"
          android:angle="270" />
      <stroke
          android:width="1dp"
          android:color="#992f2f" />
      <corners
          android:radius="4dp" />

    </shape>
  </item>
</selector>
